<?xml version="1.0" encoding="utf-8"?>
<Project>

	<PropertyGroup>
		<GrpcServices
		  Condition="'$(GrpcServices)' == '' and '@(_MvvmSourceGenPkg)' != ''">
			%(_MvvmSourceGenPkg.GrpcServices)
		</GrpcServices>
		<!-- optional trim -->
		<GrpcServices
		  Condition="'$(GrpcServices)' != ''">
			$([System.Text.RegularExpressions.Regex]::Replace($(GrpcServices), '\s+', ''))
		</GrpcServices>
	</PropertyGroup>


	<!-- your existing item declaration -->
	<ItemGroup>
		<AvailableItemName Include="MvvmViewModelProtoSource" />
	</ItemGroup>

</Project>
