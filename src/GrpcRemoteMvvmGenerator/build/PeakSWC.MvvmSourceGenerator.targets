<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<_TestPathNet8>$(MSBuildThisFileDirectory)..\tools\net8.0\ProtoGeneratorUtil.exe</_TestPathNet8>
		<_TestPathNet6>$(MSBuildThisFileDirectory)..\tools\net6.0\ProtoGeneratorUtil.exe</_TestPathNet6>

		<_ProtoGeneratorUtilExePath Condition="Exists('$(_TestPathNet8)')">$(_TestPathNet8)</_ProtoGeneratorUtilExePath>
		<_ProtoGeneratorUtilExePath Condition="Exists('$(_TestPathNet6)') AND '$(_ProtoGeneratorUtilExePath)' == ''">$(_TestPathNet6)</_ProtoGeneratorUtilExePath>
	</PropertyGroup>

	<Target Name="PeakSWC_MinimalTestTarget">
		<Message Text="Minimal PeakSWC.MvvmSourceGenerator.targets loaded. _ProtoGeneratorUtilExePath = '$(_ProtoGeneratorUtilExePath)'" Importance="high" />
	</Target>

</Project>