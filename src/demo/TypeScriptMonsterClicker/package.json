{
  "name": "typescriptmonsterclicker",
  "version": "1.0.0",
  "main": "wwwroot/app.js",
  "scripts": {
    "protoc": "protoc --plugin=protoc-gen-ts=\".\\node_modules\\.bin\\protoc-gen-ts.cmd\" --plugin=protoc-gen-grpc-web=\".\\node_modules\\protoc-gen-grpc-web\\bin\\protoc-gen-grpc-web.exe\" --js_out=\"import_style=commonjs,binary:./src/generated\" --grpc-web_out=\"import_style=typescript,mode=grpcwebtext,service_suffix=ClientPb:./src/generated\" -Iprotos -I.\\protoc\\include GameViewModelService.proto",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:debug": "webpack --mode development",
    "build:production": "npm run protoc && webpack --mode production --stats-error-details",
    "clean": "tsc --build --clean",
    "prebuild": "npm run protoc",
    "start": "webpack serve --mode development",
    "dev": "webpack serve --mode development --open",
    "watch": "webpack --watch --mode development",
    "debug": "echo \"Make sure to start the MonsterClicker server in 'server' mode before running this command\" && webpack serve --mode development --open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "protoc": "^1.1.3",
    "protoc-gen-grpc-web": "^1.5.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "3.15.12",
    "@types/node": "^24.0.7",
    "eslint": "^9.30.0",
    "google-protobuf": "3.21.4",
    "grpc-web": "^1.5.0",
    "ts-loader": "^9.5.2",
    "ts-protoc-gen": "0.15.0",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
